{
  "name": "tagpro-catstats",
  "description": "lambda's catstat collector",
  "version": "0.0.4",
  "manifest_version": 2,
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["http://*.koalabeast.com/*", "http://*.jukejuice.com/*"],
      "js": ["main.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "js/catstats.js"
  ]
}
